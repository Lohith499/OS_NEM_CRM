                        <div class="dropdown-menu" aria-labelledby="dropdown02">
                            {{#each custom_menu}}
                                    <a class="dropdown-item" href="/s/{{this}}/list">{{this}}</a>
                            {{/each}}
                        </div>
                    </li>
