<div class="Maintitle" style="margin : 10px;border-bottom: black;border-bottom-width: 5px">
    <table>
        <tr>
            {{{imgicon title}}}
            <td><h4>{{title}}</h4></td></tr></td>
    </table>

</div>

{{#if error}}
    <div class="alert alert-danger" role="alert">
        <strong style="text-transform: uppercase;">Error : </strong> {{error.sqlMessage}}
    </div>
{{/if}}


<div class="Maintitle">
    <div style="text-align: center">
        <button id="new_bUsers" class="form_button" onclick="window.location.pathname = 'setup/bUsers/new'"
                style="position: center;background:green;">New</button>
    </div>

    <table id="userlist" class="example table cell-border table-bordered hover" style="width:100%">
        <thead>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Orgnaisation_ID</th>
        </thead>
       {{#each uresult}}
           <tr>
               <td> <a href="/setup/bUsers/details?id={{this.id}}">{{this.id}}</td>
               <td> {{this.username}}</td>
               <td> {{this.email}}</td>
               <td> {{this.organisationId}}</td>
           </tr>
       {{/each}}

        <tfoot>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Orgnaisation_ID</th>
        </tfoot>
    </table>
</div>